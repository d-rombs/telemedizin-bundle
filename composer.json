{
    "name": "telemedizin/telemedizin-bundle",
    "description": "Ein wiederverwendbares Laravel-Paket f√ºr Telemedizin-Anwendungen",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Telemedizin Developer",
            "email": "dev@telemedizin.example"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "^2.0",
        "laravel/legacy-factories": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Telemedizin\\TelemedizinBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Telemedizin\\TelemedizinBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Telemedizin\\TelemedizinBundle\\TelemedizinServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 